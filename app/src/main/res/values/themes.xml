<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Androidrestaurant" parent="Theme.AppCompat.DayNight">
        <!-- Font family -->
        <item name="android:fontFamily">@font/advent_pro</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/light_steel_blue</item>
        <item name="colorOnPrimary">@color/dark_slate_gray</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/azure</item>
        <item name="colorSecondaryVariant">@color/honeydew</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/black</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ThemeOverlay.ActionBar" parent="">
        <item name="colorSurface">@color/red</item>
    </style>
</resources>