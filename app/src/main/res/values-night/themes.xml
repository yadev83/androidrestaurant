        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/light_sky_blue</item>
        <item name="colorPrimaryVariant">@color/light_sea_green</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/powder_blue</item>
        <item name="colorSecondaryVariant">@color/lighter_steel_blue</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/black</item>
        <!-- Customize your theme here. -->
    </style>
</resources>